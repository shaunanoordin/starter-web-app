{
  "name": "starter-web-app",
  "version": "0.0.9",
  "description": "Starter template for HTML5 + JavaScript + Sass web app projects.",
  "homepage": "https://github.com/shaunanoordin/starter-web-app/",
  "author": "Shaun A. Noordin (https://www.shaunanoordin.com/)",
  "main": "app/index.html",
  "repository": {
    "type": "git",
